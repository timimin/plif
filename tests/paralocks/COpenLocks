COpenLocks([role5 |-> "cor"],
		     [role5 |-> {"role4",  "role3", "role2"},
		      role4 |-> {"role3", "role2"},
		      role3 |-> {"role3"},
		      rol2 |->  {"role2"}])

RES:

[role5 |-> "cor", role4 |-> "cor", role3 |-> "cor", role2 |-> "cor"]

