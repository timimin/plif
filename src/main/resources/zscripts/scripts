CREATE TABLE "CONF"."PAPERS" 
   ("PAPER_ID" NUMBER NOT NULL ENABLE, 
	"TITLE" VARCHAR2(1000 BYTE) NOT NULL ENABLE, 
	"ABSTRACT" VARCHAR2(4000 BYTE), 
	"TEXT" CLOB NOT NULL ENABLE, 
	"AUTHORS" VARCHAR2(1000 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "PAPERS_PK" PRIMARY KEY ("PAPER_ID")
     USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
     TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
   PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
   NOCOMPRESS LOGGING
   TABLESPACE "USERS" 
   LOB ("TEXT") STORE AS SECUREFILE (
   TABLESPACE "USERS" ENABLE STORAGE IN ROW CHUNK 8192
   NOCACHE LOGGING  NOCOMPRESS  KEEP_DUPLICATES);
   
  CREATE TABLE "CONF"."SUBMISSIONS" 
   ("SUBMISSION_ID" NUMBER NOT NULL ENABLE, 
	"PAPER_ID" NUMBER NOT NULL ENABLE, 
	"CONFERENCE_ID" NUMBER NOT NULL ENABLE, 
	"SUBMISSION_DATE" DATE NOT NULL ENABLE, 
	"STATUS" NUMBER NOT NULL ENABLE, 
	 CONSTRAINT "SUBMISSIONS_PK" PRIMARY KEY ("SUBMISSION_ID")
     USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
     TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "SUBMISSIONS_FK1" FOREIGN KEY ("SUBMISSION_ID")
	     REFERENCES "CONF"."PAPERS" ("PAPER_ID") ENABLE, 
	 CONSTRAINT "SUBMISSIONS_FK2" FOREIGN KEY ("CONFERENCE_ID")
	     REFERENCES "CONF"."CONFERENCES" ("CONFERENCE_ID") ENABLE
   ) SEGMENT CREATION DEFERRED 
     PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
     NOCOMPRESS LOGGING
     TABLESPACE "USERS" ;
	 
CREATE TABLE "CONF"."CONFERENCES" 
   ("CONFERENCE_ID" NUMBER NOT NULL ENABLE, 
	"NAME" VARCHAR2(1000 BYTE) NOT NULL ENABLE, 
	"START_DATE" DATE NOT NULL ENABLE, 
	"END_DATE" DATE NOT NULL ENABLE, 
	"DESCRIPTION" VARCHAR2(4000 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "CONFERENCES_PK" PRIMARY KEY ("CONFERENCE_ID")
     USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
     TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
     PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
     NOCOMPRESS LOGGING
     TABLESPACE "USERS" ;
	 
CREATE TABLE "CONF"."SECTIONS" 
   ("SECTION_ID" NUMBER NOT NULL ENABLE, 
	"CONFERENCE_ID" NUMBER NOT NULL ENABLE, 
	"TITLE" VARCHAR2(1000 BYTE) NOT NULL ENABLE, 
	"PLACE" VARCHAR2(1000 BYTE) NOT NULL ENABLE, 
	"END_DATE" DATE NOT NULL ENABLE, 
	"START_DATE" DATE NOT NULL ENABLE, 
	 CONSTRAINT "SECTIONS_PK" PRIMARY KEY ("SECTION_ID")
     USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
     TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "SECTIONS_FK1" FOREIGN KEY ("CONFERENCE_ID")
	      REFERENCES "CONF"."CONFERENCES" ("CONFERENCE_ID") ENABLE
   ) SEGMENT CREATION DEFERRED 
     PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
     NOCOMPRESS LOGGING
     TABLESPACE "USERS" ;
	 
CREATE TABLE "CONF"."ALLOCATIONS" 
   ("ALLOCATION_ID" NUMBER NOT NULL ENABLE, 
	"SUBMISSION_ID" NUMBER NOT NULL ENABLE, 
	"SECTION_ID" NUMBER NOT NULL ENABLE, 
	"ALLOCATION_DATE" DATE NOT NULL ENABLE, 
	 CONSTRAINT "ALLOCATIONS_PK" PRIMARY KEY ("ALLOCATION_ID")
     USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
     TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "ALLOCATIONS_FK1" FOREIGN KEY ("SECTION_ID")
		REFERENCES "CONF"."SECTIONS" ("SECTION_ID") ENABLE, 
	 CONSTRAINT "ALLOCATIONS_FK2" FOREIGN KEY ("SUBMISSION_ID")
		REFERENCES "CONF"."SUBMISSIONS" ("SUBMISSION_ID") ENABLE
   ) SEGMENT CREATION DEFERRED 
     PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
	 NOCOMPRESS LOGGING
     TABLESPACE "USERS" ;

CREATE TABLE "CONF"."LOGS" 
   ("EVENT_ID" NUMBER NOT NULL ENABLE, 
	"ER_INFO" VARCHAR2(1000 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "LOGS_PK" PRIMARY KEY ("EVENT_ID")
     USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
     TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
     PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
     NOCOMPRESS LOGGING
     TABLESPACE "USERS" ;
	 
